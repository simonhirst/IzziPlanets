{
  "name": "izziplanets",
  "version": "0.18.0",
  "private": true,
  "type": "module",
  "description": "Interactive solar system to universe scale web application",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:visual": "playwright test",
    "lighthouse": "lhci autorun --config=lighthouserc.json",
    "verify": "npm run lint && npm run typecheck && npm run test && npm run build"
  },
  "keywords": [
    "threejs",
    "astronomy",
    "webgl",
    "solar-system"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "three": "0.182.0",
    "web-vitals": "5.1.0"
  },
  "devDependencies": {
    "@eslint/js": "9.39.2",
    "@lhci/cli": "0.15.1",
    "@playwright/test": "1.58.2",
    "@types/three": "0.182.0",
    "@typescript-eslint/eslint-plugin": "8.55.0",
    "@typescript-eslint/parser": "8.55.0",
    "eslint": "9.39.2",
    "prettier": "3.8.1",
    "typescript": "5.9.3",
    "vite": "7.3.1",
    "vitest": "4.0.18"
  }
}
